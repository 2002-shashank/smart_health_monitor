    {
        "id": "395a7fd9f5b6f2cd",
        "type": "function",
        "z": "1cb9f33ec81e6238",
        "name": "Check Alerts",
        "func": "let vitals = flow.get(\"vitals\") || {};\nvitals[msg.vital] = msg.payload;\nflow.set(\"vitals\", vitals);\n\nif (Object.keys(vitals).length === 4) {\n    let alerts = [];\n\n    if (vitals.heart_rate < 60 || vitals.heart_rate > 160)\n        alerts.push(`ð¨ Heart Rate: ${vitals.heart_rate} bpm`);\n\n    if (vitals.systolic < 90 || vitals.systolic > 160)\n        alerts.push(`ð¨ Systolic BP: ${vitals.systolic} mmHg`);\n\n    if (vitals.diastolic < 60 || vitals.diastolic > 100)\n        alerts.push(`ð¨ Diastolic BP: ${vitals.diastolic} mmHg`);\n\n    if (vitals.temperature < 35 || vitals.temperature > 40)\n        alerts.push(`ð¨ Temperature: ${vitals.temperature} Â°C`);\n\n    flow.set(\"vitals\", {}); // reset for next cycle\n\n    if (alerts.length > 0) {\n        return { payload: alerts.join(\"\\n\") };\n    }\n}\n\nreturn null;\n",
